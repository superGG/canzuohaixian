<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
        
<hibernate-mapping package="com.earl.fishshop.pojo">
    <class name="FishmanPo" table="Fishman" dynamic-update="true" dynamic-insert="true">
        <id name="fishmanId" type="java.lang.Long">
            <column name="fishmanId" />
            <generator class="native"></generator>
        </id>
        <property name="userType" type="java.lang.Integer" column="userType" />
        <property name="identityPhoto" type="java.lang.String" column="identityPhoto" />
        <property name="seaPassPhoto" type="java.lang.String" column="seaPassPhoto" />
        <property name="createTime" type="java.util.Date" column="createTime" />
        <property name="creatorId" type="java.lang.Long" column="creatorId" />
        <property name="isDelete" type="java.lang.Boolean" column="isDelete" />
        <property name="version" type="java.lang.Long" column="version" />
    </class>
</hibernate-mapping>
