<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.earl.fishshop.pojo">
 <class dynamic-insert="true" dynamic-update="true" name="Goods" table="goods">
  <id name="id" type="java.lang.Integer">
   <column name="id"/>
   <generator class="native"/>
  </id>
  <property column="goodsname" generated="never" lazy="false"
   name="goodsname" type="java.lang.String"/>
  <property column="description" generated="never" lazy="false"
   name="description" type="java.lang.String"/>
  <property column="goodsattributes" generated="never" lazy="false"
   name="goodsattributes" type="java.lang.String"/>
  <property column="goodsprice" generated="never" lazy="false"
   name="goodsprice" type="java.lang.Float"/>
  <property column="goodspic" generated="never" lazy="false"
   name="goodspic" type="java.lang.String"/>
  <property column="ishot" generated="never" lazy="false" name="ishot" type="java.lang.Boolean"/>
  <property column="goodsbrand" generated="never" lazy="false"
   name="goodsbrand" type="java.lang.String"/>
  <property column="goodstype" generated="never" lazy="false"
   name="goodstype" type="java.lang.String"/>
  <property column="manufacturer" generated="never" lazy="false"
   name="manufacturer" type="java.lang.String"/>
 </class>
</hibernate-mapping>
