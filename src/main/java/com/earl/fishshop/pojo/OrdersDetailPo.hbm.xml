<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
        
<hibernate-mapping package="com.earl.fishshop.pojo">
    <class name="OrdersDetailPo" table="ordersDetail" dynamic-update="true" dynamic-insert="true">
        <id name="ordersDetailId" type="java.lang.Long">
            <column name="ordersDetailId" />
            <generator class="native"></generator>
        </id>
        <version name="version" column="version" type="java.lang.Long"></version>
        <property name="orderId" type="java.lang.Long" column="orderId" />
        <property name="shopId" type="java.lang.Long" column="shopId" />
        <property name="goodsId" type="java.lang.Long" column="goodsId" />
        <property name="categoryId" type="java.lang.Long" column="categoryId" />
        <property name="goodsName" type="java.lang.String" column="goodsName" />
        <property name="goodsType" type="java.lang.String" column="goodsType" />
        <property name="price" type="java.lang.Double" column="price" />
        <property name="totalprice" type="java.lang.Double" column="totalprice" />
        <property name="sku" type="java.lang.String" column="sku" />
        <property name="unit" type="java.lang.String" column="unit" />
        <property name="sellNumber" type="java.lang.Long" column="sellNumber" />
        <property name="number" type="java.lang.Long" column="number" />
        <property name="createTime" type="java.util.Date" column="createTime" />
        <property name="creatorId" type="java.lang.Long" column="creatorId" />
        <property name="isDelete" type="java.lang.Boolean" column="isDelete" />
    </class>
</hibernate-mapping>
